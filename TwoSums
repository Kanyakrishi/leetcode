def twoSum(nums, target):
    flag = 0
    if not flag:
        for num1 in range(0, len(nums) - 1):
            for num2 in range(num1 + 1, len(nums)):
                if nums[num1] + nums[num2] == target:
                    print(list([num1, num2]))
                    flag = 1

twoSum([3,2,4], 6)


def rotate(arr, n):
    t = arr[-1]
    for i in range(n-1,1):
        arr[i] = arr[i-1]
        i -= 1
    arr[0] = t


# {
#  Driver Code Starts
# Initial Template for Python 3
a = [1, 2, 3, 4, 5]
rotate(a, 5)
print(*a)

